                    A1w2p4:
Input = input("Sides: ")
Parts = Input.replace(" ", "").split(",")

sides = {}
for part in Parts:
    key, value = part.split("=")
    sides[key] = int(value)
    
A = sides.get("a")
B = sides.get("b")
C = sides.get("c")

if A == B == C:
    print("Equilateral")
elif (A==B!=C) or (A!=B==C) or (A==C!=B):
    print("Isosceles")
else:
    print("Scalene")

