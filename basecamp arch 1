word = input("give the word: ").lower()

x = list(word)

rev =[]

for i in range(len(x) -1, -1, -1):
    rev.append(x[i])
    y = "".join(rev)



if y == word:
    print(f'{word} is a palindrome')
else:
    print(f'{word} is not a palindrome')
    
