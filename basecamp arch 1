                    A1w3p7:
def And(a: bool, b: bool) ->bool:
    if  b and a:
        print(f'{a} + {b} = {True}')
    else:
        print(f'{a} + {b} = {False}')
    
def Or(a: bool, b: bool) ->bool:
    if  not b or not a:
        print(f'{a} + {b} = {True}')
    else:
        print(f'{a} + {b} = {False}')
Value = [True, False]

for a in Value:
    for b in Value:
        
        And(a,b)
        

for a in Value:    
    for b in Value:        
        Or(a,b)
