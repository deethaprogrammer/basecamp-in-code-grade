                    A1w2a1:
def validate_float(input_str: str) -> bool:
    try:
        float(input_str)
        return True
    except ValueError:
        return False

def validate_int(input_str: str) -> bool:
    try:
        int(input_str)
        return True
    except ValueError:
        return False


subscription_price = input('subscription_price: ')
if not validate_float(subscription_price):
    print("Invalid input")
    exit()

single_price = input("single ticket price: ")
if not validate_float(single_price):
    print("Invalid input")
    exit()

visits = input("amount of visits: ")
if not validate_int(visits):
    print("Invalid input")
    exit()
    
subscription_price = float(subscription_price)
single_price = float(single_price)
visits = int(visits)

max_ticket = visits * single_price
saved = max_ticket - subscription_price

if max_ticket > subscription_price:
   print(f"Single tickets: €{max_ticket: .1f}\nMonthly subscription: €{subscription_price: .1f}\nAdvice: Buy a subscription\nYou save €{saved}") 
else:
    print(f"Single tickets: €{max_ticket: .1f}\nMonthly subscription: €{subscription_price: .1f}\nAdvice: Buy single tickets")
  
