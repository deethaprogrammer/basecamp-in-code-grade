                    A1w2p8:
License = input("License: ")
def is_valid_license(License):
    parts = License.lower().split("-")
    
    def is_letter(S): return all('a'<= C <= 'z' for C in S)
    def is_digit(S): return all('0' <= C <= '9' for C in S)
    
    patterns = [
        [2, 'L', 2, 'D', 2, 'D'],  # XX-99-99
        [2, 'D', 2, 'D', 2, 'L'],  # 99-99-XX
        [2, 'D', 2, 'L', 2, 'D'],  # 99-XX-99
        [2, 'L', 2, 'D', 2, 'L'],  # XX-99-XX
        [2, 'L', 2, 'L', 2, 'D'],  # XX-XX-99
        [2, 'D', 2, 'L', 2, 'L'],  # 99-XX-XX
        [2, 'D', 3, 'L', 1, 'D'],  # 99-XXX-9
        [1, 'D', 3, 'L', 2, 'D'],  # 9-XXX-99
        [2, 'L', 3, 'D', 1, 'L'],  # XX-999-X
        [1, 'L', 3, 'D', 2, 'L'],  # X-999-XX
        [3, 'L', 2, 'D', 1, 'L'],  # XXX-99-X
        [1, 'D', 2, 'L', 3, 'D']   # 9-XX-999
    ]

    for pattern in patterns:
        if len(parts) != len(pattern)//2:
            continue 
        match = True
        for i in range(len(parts)):
            expected_len = pattern[i*2]
            expected_type = pattern[i*2+1]
            if len(parts[i]) != expected_len:
                match = False
                break
            if expected_type == 'L' and not is_letter(parts[i]):
                match = False
                break
            if expected_type == 'D' and not is_digit(parts[i]):
                match = False
                break
        if match:
            return True
    return False

if is_valid_license(License):
    print("Valid")
else:
    print("Invalid")
    
                    A1w3a 1,2 first version:
def is_first_name_valid(name: str) -> bool:
    return (
        len(name) <= 10 and
        len(name) >= 2 and
        name[0].isupper() and
        name.isalpha()
    )
    
def is_last_name_valid(name: str) -> bool:
    return (
        len(name)>=2 and
        len(name)<=20 and
        all(character.isalpha() or character in " -/," for character in name)
    )
    
def is_time_valid(time_str: str) -> bool:
    time = time_str.split(":")
    if time_str.isalpha():
        return False
    if not (time[0].isdigit() and time[1].isdigit()):
        return False
    if ((len(time[0]) == 2) and (len(time[1]) == 2)) and ((0<=int(time[0])<=23) and (0<=int(time[1])<=59)):
        return True    
    return False
        

def is_duration_valid(start, end) -> bool:
    global duration
    try:
        if isinstance(start, int) and isinstance(end, int):
            duration = end - start
            return (
                duration>=10 and
                duration<=180
                )

        start = str(start)
        end = str(end)
        starting = start.split(":")
        ending = end.split(":")
        
        if len(starting) !=2 or len(ending)!= 2:
            return False
        
        if not all(parts.isdigit() for parts in starting + ending):
            return False
        
        
        start_in_minute = ((int(starting[0])*60) + int(starting[1]))
        end_in_minute = ((int(ending[0])*60) + int(ending[1]))
        if end_in_minute < start_in_minute:
            return False
    except ValueError:
        return False
    
    duration = end_in_minute - start_in_minute
    return (
        duration>=10 and
        duration<=180
    )

def main():
    
    global First_Name, Last_Name, Start_Time, Finish_Time, people, total_duration, fastest_name, fastest_time, full_name, FH, FM, AH, AM
    fastest_name = ""
    people = 0
    total_duration = 0
    fastest_time = 9999
    yes = True
    while yes:
        
        while True:
            First_Name = input("First name: ")
            if is_first_name_valid(First_Name):
                break
            print("input error")
        while True:
            Last_Name = input("Last name: ")
            if is_last_name_valid(Last_Name):
                break
            print("input error")
        while True:
            while True:
                Start_Time = input("Start time: ")
                if is_time_valid(Start_Time):
                    break
                print("input error")
            while True:
                Finish_Time = input("Finish time: ")
                if is_time_valid(Finish_Time):
                    break
                print("input error")
                
            if not is_duration_valid(Start_Time, Finish_Time):
                print("input error")
            else:
                break
            
        full_name = First_Name + '_' + Last_Name
        globals()[full_name + "_duration"] = duration
        total_duration += duration
        people += 1
        DH = str(duration//60).zfill(2)
        DM = str(duration%60).zfill(2)
        if duration < fastest_time:
            fastest_time = duration
            fastest_name = First_Name + " " + Last_Name
            FH = str(fastest_time//60).zfill(2)
            FM = str(fastest_time%60).zfill(2)
        
        average = total_duration // people
        AH = str(average//60).zfill(2)
        AM = str(average%60).zfill(2)
        if duration < average:
            Speed_against_average = "faster than"
        elif duration == average:
            Speed_against_average = "as fast as"
        else:
            Speed_against_average = "slower than"
        print(f'{First_Name} {Last_Name} did it in {DH}:{DM}')
        print(f'This participant is currently {Speed_against_average} the average.')
        
        while True:
            new = input("Are you a new person?: ").lower()
            if new == "yes":
                break
            elif new == "no":
                yes = False
                break
            else:
                input("type: yes 'or' no")
if __name__ == "__main__":
    main()
    print(f"Fastest participant: {fastest_name} ({FH}:{FM})")
    print(f"Total participants: {people}")
    print(f"Average time: {AH}:{AM}")
    
